def categorize_age(19):
  """Categorizes a user's age into different groups.

  Args:
    age: The user's age as an integer.

  Returns:
    A string representing the age category.
  """

  if age < 5:
    return "Toddler"
  elif age <= 12:
    return "Child"
  elif age <= 19:
    return "Teenager"
  elif age <= 35:
    return "Young Adult"
  elif age <= 60:
    return "Middle-Aged"
  else:
    return "Senior"

# Get the user's age
user_age = int(input("19: "))

# Categorize the age and print the result
age_category = categorize_age(user_age)
print("You are a", age_category)
